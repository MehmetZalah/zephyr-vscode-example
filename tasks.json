{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "West Build",
			"type": "shell",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"command": "${userHome}/code/zephyrproject/.venv/Scripts/west.exe",
			"args": [
				"build",
				"-p",
				"auto",
				"-b",
				"adafruit_feather_m0_basic_proto"
			],
			"problemMatcher": [
				"$gcc"
			]
		},
		{
			"label": "West Flash",
			"type": "shell",
			"command": "${userHome}/code/zephyrproject/.venv/Scripts/west.exe",
			"args": [
				"flash"
			],
			"problemMatcher": [
				"$gcc"
			]
		}
	]
}
